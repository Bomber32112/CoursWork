<Window x:Class="CoursWork.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoursWork"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListView ItemsSource = "{Binding TestTest}">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="ID" DisplayMemberBinding="{Binding ID}"/>
                    <GridViewColumn Header="String" DisplayMemberBinding="{Binding TestString}"/>
                    <GridViewColumn Header="DateTime" DisplayMemberBinding="{Binding TestDateTime}"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
